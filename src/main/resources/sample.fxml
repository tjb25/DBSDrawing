<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.canvas.Canvas?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="com.softserve.shapes.ControllerImpl">
    <children>
        <Canvas height="512" width="512" fx:id="canvas"/>
        <HBox spacing="10">
            <Label text="Enter Command:" fx:id="label1">
                <font>
                    <Font name="Arial" size="12.0" />
                </font>
            </Label>
            <TextField onKeyPressed="#applyKeyPressed" fx:id="textField">
                <font>
                    <Font name="Arial" size="12.0" />
                </font>
            </TextField>
            <Label text="" fx:id="errorLabel">
                <font>
                    <Font name="Arial" size="12.0" />
                </font>
            </Label>
        </HBox>
    </children>
</VBox>